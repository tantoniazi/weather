<div id="<%= dom_id weather %>">
  <p>
    <strong>Temperature:</strong>
    <%= weather.temperature %>
  </p>

  <p>
    <strong>Temp min:</strong>
    <%= weather.temp_min %>
  </p>

  <p>
    <strong>Temp max:</strong>
    <%= weather.temp_max %>
  </p>

  <p>
    <strong>Description:</strong>
    <%= weather.description %>
  </p>

</div>
